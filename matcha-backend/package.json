{
  "name": "matcha_backend",
  "version": "1.0.0",
  "description": "expressjs typescript template",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --config ./nodemon.json src/index.ts",
    "build": "tsc --project tsconfig.build.json",
    "buildstart": "npm run build && node build/index.js",
    "start1": "tsc --noEmit && node --loader ts-node/esm/transpile-only src/index.ts",
    "start": "tsc --noEmit && tsx src/index.ts",
    "prebuild": "test -f ./logs/app.log || mkdir -p ./logs && touch ./logs/app.log",
    "serve": "node build/index.js",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.41.1",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "neo4j-driver": "^5.28.1",
    "nodemailer": "^7.0.6",
    "pg": "^8.13.0",
    "pino": "^9.4.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.7.4",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.7",
    "pino-pretty": "^11.2.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  }
}
